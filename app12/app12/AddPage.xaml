<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="app12.AddPage" Title="Добавить">
    <ContentPage.Content>
        <StackLayout Margin="10,10,10,0">
            <Label Text="ФИО"></Label>
            <Entry x:Name="fioText"></Entry>
            <Label Text="Улица"></Label>
            <Entry x:Name="streetText"></Entry>
            <Label Text="Дом"></Label>
            <Entry x:Name="houseText"></Entry>
            <Label Text="Квартира"></Label>
            <Entry x:Name="flatText"></Entry>
            <Button Text="Добавить" Clicked="Add_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>